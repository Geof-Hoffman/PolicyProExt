{
    "name": "Policy Pro",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "128": "128.png",
        "48": "48.png",
        "16": "16.png"
      },
    
      "browser_action": {
        "default_icon": "16.png",
        "default_popup": "popup.html"
      },
      "permissions": ["storage"],
      "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js":["popup.js"]
        }
        
    ]
}
